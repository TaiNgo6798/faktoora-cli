{
  "name": "@faktoora/faktoora-cli",
  "version": "1.0.12",
  "bin": {
    "faktoora": "dist/index.js"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "lint": "eslint src/ --ext .ts",
    "lint-fix": "eslint --fix src --ext ts",
    "prepare": "husky",
    "dev": "nodemon ./src/index.ts",
    "start": "node ./dist/index.js",
    "test": "jest --no-cache --forceExit",
    "clean": "rimraf coverage dist tmp",
    "copy-files": "copyfiles -u 1 -s 'src/**/!(*.ts|*.js|*.map|*.mjs)' dist",
    "build": "npm run lint-fix && npm run clean && tsc -p tsconfig.json && npm run copy-files",
    "postinstall": "chmod +x dist/index.js"
  },
  "dependencies": {
    "axios": "^1.7.5",
    "commander": "^12.1.0",
    "inquirer": "^10.1.8",
    "simple-git": "^3.25.0"
  },
  "devDependencies": {
    "@types/vinyl": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.8.1",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "junit": "^1.4.9",
    "nodemon": "^3.1.4",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.4"
  }
}
